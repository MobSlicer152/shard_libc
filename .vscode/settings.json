{
	"editor.autoIndent": "advanced",
	"editor.detectIndentation": false,
	"editor.tabSize": 8,

	"[json]": {
		"editor.quickSuggestions": {
			"strings": true
		},
		
		"editor.tabSize": 4
	},
	
	"editor.insertSpaces": false,
	"editor.tabCompletion": "on",
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/node_modules/**": true,
		"**/.hg/store/**": true,
		"**/deps/**": true
	},
	"files.eol": "\n"
}
